<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<class
		name="com.wastedge.api.jasper.adapter.WEAdapterImpl">
		<map-to xml="WEAdapter" />
		<field name="class" type="java.lang.Class"
			handler="net.sf.jasperreports.data.ClassNameFieldHandler">
			<bind-xml name="class" node="attribute" />
		</field>
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="wastedgeHost" type="string">
			<bind-xml name="wastedgeHost" node="element" />
		</field>
		<field name="wastedgeCompany" type="string">
			<bind-xml name="wastedgeCompany" node="element" />
		</field>
		<field name="wastedgeUsername" type="string">
			<bind-xml name="wastedgeUsername" node="element" />
		</field>
		<field name="wastedgePassword" type="string">
			<bind-xml name="wastedgePassword" node="element" />
		</field>
	</class>
</mapping>
